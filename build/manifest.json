{
  "manifest_version": 3,
  "name": "KV.featureFlag",
  "short_name": "KV.featureFlag",
  "description": "Chrome extension для работы с функционалом скрытым под feature flag",
  "version": "1.0",
  "icons": {
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "96": "images/icon-96.png"
  },
  "action": {
    "default_icon": {
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "96": "images/icon-96.png"
    },
    "default_title": "KV.featureFlag",
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "activeTab", "storage"],
  "author": "Kirill Vanyukov",
  "homepage_url": "https://github.com/vanyukov"
}
